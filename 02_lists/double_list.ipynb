   "source": [
    "number_list: list[int] = [2,8,9,7,10,15]\n",
    "\n",
    "def main():\n",
    "    for num in range(len(number_list)):\n",
    "        elem_at_index = number_list[num]\n",
    "        number_list[num] = elem_at_index * 2\n",
    "\n",
    "    print(number_list)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  }
