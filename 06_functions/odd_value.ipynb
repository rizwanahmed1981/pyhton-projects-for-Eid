   "source": [
    "# Odd number Extractor"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 is odd number\n",
      "3 is odd number\n",
      "5 is odd number\n",
      "7 is odd number\n",
      "9 is odd number\n",
      "11 is odd number\n",
      "13 is odd number\n",
      "15 is odd number\n",
      "17 is odd number\n",
      "19 is odd number\n"
     ]
    }
   ],
   "source": [
    "\n",
    "LIMIT = 20\n",
    "\n",
    "def odd_extractor():\n",
    "    for num in range(LIMIT):\n",
    "        if num % 2 != 0:\n",
    "            print(f\"{num} is odd number\")\n",
    "\n",
    "def main():\n",
    "    odd_extractor()\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  }
